<!DOCTYPE html>
<html lang="es">
  <%- include('./partials/head') %>

  <body>
    <%- include('./partials/header') %>
    <main class="todasLasFotos">
        <h1 >           Fotos
        </h1>
        <section>
          <% fotosData.forEach(element => { %>
            <a class="card" href="detailFoto/<%= element.id %>">
              <img  src="images/<%= element.fotos[0] %>" alt="">
              <h2><%= element.titulo %></h2>
              <p><%= element.contenido %></p>
            </a>
           
          <% }) %>
        </section>
    </main>

    <%- include('./partials/footer') %>   
   
  
    
    <%- include('./partials/scripts') %>
  </body>
</html>
